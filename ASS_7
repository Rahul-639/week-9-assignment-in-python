def zeroSumSubarrays(arr):
    prefix_sum = 0
    freq = {0: 1}
    count = 0
    for num in arr:
        prefix_sum += num
        if prefix_sum in freq:
            count += freq[prefix_sum]
            freq[prefix_sum] += 1
        else:
            freq[prefix_sum] = 1
    return count
print(zeroSumSubarrays([0, 0, 5, 5, 0, 0]))  # 6
print(zeroSumSubarrays([6, -1, -3, 4, -2, 2, 4, 6, -12, -7]))  # 4
